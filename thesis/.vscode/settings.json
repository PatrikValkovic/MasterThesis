{
    "files.exclude": {
        "**/*.aux": true,
        "**/*.synctex*": true,
        "creationdate.lua": true,
        "creationdate.timestamp": true,
        "thesis.acn": true,
        "thesis.acr": true,
        "thesis.alg": true,
        "thesis.bbl": true,
        "thesis.blg": true,
        "thesis.fdb_latexmk": true,
        "thesis.fls": true,
        "thesis.glg": true,
        "thesis.glo": true,
        "thesis.gls": true,
        "thesis.ist": true,
        "thesis.lof": true,
        "thesis.log": true,
        "thesis.lot": true,
        "thesis.out": true,
        "thesis.toc": true,
        "thesis.xdv": true,
        "tlesis.glg": true,
